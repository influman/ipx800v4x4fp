				{ "value": "4", "description": "Confort-1", "icon": "chauffage_m1.png", "http_url": "http://[VAR1]/user/api.cgi", "http_type": "GET",  "http_params": "?Set4FP=[VAR2]&FpZone=1&FpCmd=[RAW_VALUE]" },
